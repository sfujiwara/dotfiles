        "command": "cursorHome"
    },
    {
        "key": "ctrl+e",
        "command": "cursorEnd"
    },
    {
        "key": "ctrl+d",
        "command": "deleteRight"
    },
    {
        "key": "ctrl+k",
        "command": "deleteAllRight"
